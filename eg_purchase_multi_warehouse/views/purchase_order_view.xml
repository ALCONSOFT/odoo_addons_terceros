<odoo>

    <record id="purchase_order_inherit_form_purchase_receipt_by_date" model="ir.ui.view">
        <field name="name">purchase.order.inherit.form.purchase.receipt.by.date</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='products']/field[@name='order_line']/tree/field[@name='name']" position="after">
                    <field name="picking_type_id"/>
            </xpath>
        </field>
    </record>

</odoo>
