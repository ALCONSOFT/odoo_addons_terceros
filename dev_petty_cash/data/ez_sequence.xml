<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="dev_petty_cash_request" model="ir.sequence">
            <field name="name">Petty Cash Request</field>
            <field name="code">petty.cash.request</field>
            <field name="prefix">PREQ/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="dev_petty_cash_expense" model="ir.sequence">
            <field name="name">Petty Cash Expense</field>
            <field name="code">petty.cash.expense</field>
            <field name="prefix">PEXP/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>


